{
    "name": "extpoint/yii2-megamenu",
    "description": "Configurable site map with auto generate page title, breadcrumbs and navigation",
    "keywords": ["yii2", "menu", "navigation", "breadcrumbs"],
    "license": "MIT",
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Kozhin Vladimir",
            "email": "affka@affka.ru"
        }
    ],
    "require":{
        "php":">=5.4.0",
        "yiisoft/yii2": "2.*"
    },
    "require-dev":{
        "yiisoft/yii2-bootstrap": "*",
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "extpoint\\megamenu\\": "lib/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
